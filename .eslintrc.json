{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "arrow-parens": "off",
        "no-alert":"off",
        "quotes": ["error", "double"],
        "no-use-before-define": ["error", {
            "functions": false,
            "classes": false,
            "variables": false
     }]
    }
}

